{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"outDir": "../dist",
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"$lib/*": [
				"src/lib/*"
			],
			"$lib": [
				"src/lib"
			],
			"$stores": [
				"src/stores"
			],
			"$stores/*": [
				"src/stores/*"
			]
		}
	}
}